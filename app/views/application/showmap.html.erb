
<div id="map_canvas" style="width:100%; height:100%"></div>

<script>
var map = init_map("map_canvas",
                   <%= @location.latitude %>,
                   <%= @location.longitude %>);

<% @fixmystreet.size.times do |i| %>
	<% coord = @fixmystreet[i].geo.split(' ') %>
    pin_event(map,
              new Event("<%= @fixmystreet[i].title %>",
                        <%= coord[0] %>,
                        <%= coord[1] %>));
<% end %>
</script>
